<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="mark.fang.platform.domain.Media" table="media">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		
		<property name="pid" column="pid"></property>
		<property name="sid" column="sid"></property>
		<property name="rootid" column="rootid"></property>
		<property name="title" column="title"></property>
		<property name="detail" column="detail"></property>
		<property name="author" column="author"></property>
		<property name="reviews" column="reviews"></property>
		<property name="replies" column="replies"></property>
		<property name="pdate" column="pdate"></property>
		<property name="file" column="file"></property>
		<property name="filetype" column="filetype"></property>
		<property name="fileimage" column="fileimage"></property>
	</class>
</hibernate-mapping>